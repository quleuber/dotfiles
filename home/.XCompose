# Import default rules from the system Compose file
include "%L"

# <Multi_key> <> # DAGGER

<Multi_key> <v> <v> : "✓" U2713
<Multi_key> <exclam> <question> : "‽"   U203D  # INTERROBANG
<Multi_key> <less>    <period>  : "‹"   U2039  # LEFT-POINTING ANGLE QUOTATION MARK
<Multi_key> <greater> <period>  : "›"   U203A  # RIGHT-POINTING ANGLE QUOTATION MARK


# ARROWS
<Multi_key> <less> <minus> <greater>      : "↔"  U2194
<Multi_key> <downarrow> <minus> <uparrow> : "↕"  U2195
<Multi_key> <uparrow> <minus> <downarrow> : "↕"  U2195
### TEST ###
<Multi_key> <bar> <downarrow> : "↓"  U2193
<Multi_key> <bar> <uparrow>   : "↑"  U2191
# LONG ARROWS
<Multi_key> <less> <minus> <minus> :    "⟵"  U27F5
<Multi_key> <minus> <minus> <greater> : "⟶"  U27F6
<Multi_key> <less> <minus> <minus> <greater> : "⟷"  U27F7


# MATH
<Multi_key> <minus> <period> : "−"
<Multi_key> <minus> <plus> : "∓"
<Multi_key> <asciicircum> <period> : "⋀"
<Multi_key> <backslash> <slash> : "⋁"
# SET THEORY
<Multi_key> <S> <e> <minus> : "∊"
<Multi_key> <S> <E> <minus> : "∈"
<Multi_key> <V> <minus> : "∀"
<Multi_key> <S> <V> <minus> : "∀"
<Multi_key> <E> <E> : "∃"
# LOGIC
<Multi_key> <less> <equal> : "⇐"
<Multi_key> <equal> <greater> : "⇒"
<Multi_key> <less> <equal> <greater> : "⇔"

